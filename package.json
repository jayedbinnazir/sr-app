{
  "name": "my-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nodemon --watch src --ext ts --exec 'ts-node -r tsconfig-paths/register src/main.ts' --legacy-watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mgd": "cross-env NODE_ENV=dev npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/typeorm.datasource.ts",
    "mc": "cross-env NODE_ENV=dev npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./migrations",
    "mr": "cross-env NODE_ENV=dev npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/database/typeorm.datasource.ts",
    "mrv": "cross-env NODE_ENV=dev npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/database/typeorm.datasource.ts",
    "ms": "cross-env NODE_ENV=dev npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show -d ./src/database/typeorm.datasource.ts",
    "docker:build": "docker-compose -f docker-compose.dev.yml --env-file .env.dev up --build",
    "docker:service": "docker-compose -f docker-compose.service.yml --env-file .env.dev up -d",
    "docker:kafka": "docker-compose -f docker-compose.kafka.yml --env-file .env.dev up --build",
    "docker:down": "docker-compose -f docker-compose.dev.yml --env-file .env.dev down",
    "docker:down:v": "docker-compose -f docker-compose.dev.yml --env-file .env.dev down -v",
    "docker:down:orphans": "docker-compose -f docker-compose.dev.yml --env-file .env.dev down --remove-orphans",
    "docker:db:mr": "docker-compose -f docker-compose.dev.yml --env-file .env.dev exec app1 npm run mr",
    "docker:db:mgd": "docker-compose -f docker-compose.dev.yml --env-file .env.dev exec app1 npm run mgd",
    "docker:db:mrv": "docker-compose -f docker-compose.dev.yml --env-file .env.dev exec app1 npm run mrv",
    "docker:db:ms": "docker-compose -f docker-compose.dev.yml --env-file .env.dev exec app1 npm run ms"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.8",
    "@socket.io/redis-adapter": "^8.3.0",
    "cache-manager": "^7.2.4",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "kafkajs": "^2.2.4",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "rsa-pem-to-jwk": "^1.1.3",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/cache-manager-ioredis": "^2.0.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.13",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.27",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
